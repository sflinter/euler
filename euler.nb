Cell["Euler  34", "Section",
 CellChangeTimes->{{3.4519359778441772`*^9, 3.451935983306385*^9}}],

Cell["\<\
145 is a curious number, as 1! + 4! + 5! = 1 + 24 + 120 = 145.

Find the sum of all numbers which are equal to the sum of the factorial of \
their digits.Note : as 1! = 1 and 2! = 2 are not sums they are not included.\
\>", "Text"],

Cell[BoxData[
 RowBox[{"euler34", ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"SumOfFactorials", ",", "sum"}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{
      RowBox[{"SumOfFactorials", "[", "n_", "]"}], ":=", 
      RowBox[{"Total", "[", 
       RowBox[{"Factorial", "/@", 
        RowBox[{"IntegerDigits", "[", "n", "]"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"sum", "=", "0"}], ";", "\[IndentingNewLine]", 
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"i", "=", "3"}], ",", 
       RowBox[{"i", "<", "100000"}], ",", 
       RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"SumOfFactorials", "[", "i", "]"}], "\[Equal]", "i"}], ",", 
         
         RowBox[{"sum", "+=", "i"}]}], "]"}]}], "\[IndentingNewLine]", "]"}], 
     ";", "\[IndentingNewLine]", "sum"}]}], "\[IndentingNewLine]", 
   "]"}]}]], "Code",
